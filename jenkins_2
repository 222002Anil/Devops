pipeline {
agent any
  stages ('wellcome'){
  stage('Parallel Stage') {
       steps {
                sh 'echo "hi"'
}
            parallel {
                stage('child stage') {
                    steps {
                        sh 'echo "bye"'
}
}
}
}

    stage ('first one'){
      steps {
        sh 'echo "hello its sucessufullly running"'
          }
    }
    stage ('second one'){
      steps {
        sh 'echo "hello it is also running sucessfully"'
          }
    }
    stage ('third one'){
      steps {
         sh 'echo "thank you"'
            }
        }
    }
}
        
