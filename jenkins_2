pipeline {
agent any
  stages ('wellcome'){
    when {
      anyOf{
        branch 'main';
        branch 'dev'
          }
      }
    steps{
    sh 'echo "wellcome again"'
}

    stage ('first one'){
      steps {
        sh 'echo "hello its sucessufullly running"'
          }
    }
    stage ('second one'){
      steps {
        sh 'echo "hello it is also running sucessfully"'
          }
    }
    stage ('third one'){
      steps {
         sh 'echo "thank you"'
            }
        }
    }
}
        
